{
  "name": "bisheng-theme-one",
  "description": "The one theme for bisheng.",
  "scripts": {
    "start": "bisheng start --no-livereload",
    "deploy": "npm run babel && bisheng gh-pages --ssr",
    "babel": "babel ./_theme/template --out-dir ./_theme/template",
    "lint": "eslint ./_theme",
    "eslint-fix": "eslint --fix ./_theme",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjycui/bisheng-theme-one.git"
  },
  "keywords": [
    "bisheng",
    "theme",
    "one"
  ],
  "author": "Benjy Cui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjycui/bisheng-theme-one/issues"
  },
  "homepage": "https://github.com/benjycui/bisheng-theme-one#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1"
  },
  "dependencies": {
    "bisheng": "^0.18.0",
    "bisheng-plugin-description": "^0.1.0",
    "bluebird": "^3.4.1",
    "react": "^15.0.2",
    "react-document-title": "^2.0.1",
    "react-router": "^2.4.0"
  }
}
